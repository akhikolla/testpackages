citHeader("To cite the synlik package in publications use:")

citEntry(
         entry="Manual",
         title="An introduction to esaddle (2016). R package version 0.0.2.",
         author=personList(
 	   as.person("Matteo Fasiolo"),
	   as.person("Simon N. Wood")
	   ),	   
	 year=2016,
	 textVersion=paste(
			   "Matteo Fasiolo, Simon N. Wood.",
			   "An introduction to esaddle (2016). R package version 0.0.2."
			  )	   
	 )		 