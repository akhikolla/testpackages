year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
  title  = "{dosearch}: Causal Effect Identification from Multiple Incomplete Data Sources",
  author = c(person("Santtu", "Tikka"),
             person("Antti", "Hyttinen"),
             person("Juha", "Karvanen")),
  year = year,
  note = note,
  url = "https://cran.r-project.org/package=dosearch")

