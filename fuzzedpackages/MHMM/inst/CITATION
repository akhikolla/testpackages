         title = "Mixture of hidden Markov models for accelerometer data",
         author = personList(person(given="Marie", family={"Du Roy de Chaumaray"}),
                             person(given="Matthieu", family="Marbac"),
                             person(given="Fabien", family="Navarro")),
         journal = "arXiv preprint arXiv:1906.01547",
         year = "2019",
         #
         textVersion =
           paste("Du Roy de Chaumaray, M. and Marbac, M. and Navarro, F. (2019)",
                 "Mixture of hidden Markov models for accelerometer data,",
                 "arXiv preprint arXiv:1906.01547"))



