citHeader("To cite s2net in publications use:")

citEntry(entry = "Article",
         title        = "A generalized elastic-net for semi-supervised learning of sparse features",
         author       = personList(as.person("Juan C. Laria"),
                                   as.person("Line H. Clemmensen"),
                                   as.person("Bjarne K. Ersbol")),
         journal      = ".",
         year         = "2020",
         volume       = "[in-press]",
         number       = "",
         pages        = "",
         url          = "",
         doi          = "",

         textVersion  = paste("Laria J. et. al.",
                              '"A generalized elastic-net for semi-supervised learning of sparse features"',
                              "[in-press]",
                              "")
)