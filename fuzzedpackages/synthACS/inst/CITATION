citHeader("To cite synthACS in publications use:")

citEntry(entry = "Article",
  title        = "{synthACS}: Spatial MicroSimulation Modeling with Synthetic American Community Survey Data",
  author       = personList(as.person("Alex Whitworth")),
  journal      = "Journal of Statistical Software",
  year         = "Forthcoming",
#   volume       = "TBD",
#   number       = "TBD",
#   pages        = "TBD",
#   url          = "https://www.jstatsoft.org/v??",

  textVersion  =
  paste("Alex Whitworth.", # (2020)
        "synthACS: Spatial MicroSimulation Modeling with Synthetic American Community Survey Data.",
        "Journal of Statistical Software, Forthcoming" # XX(x), X-XX."
  )
#         "URL TBD.")
)
