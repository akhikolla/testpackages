CXX_STD=CXX11
PKG_CFLAGS=@PKG_CFLAGS@
PKG_CPPFLAGS=@PKG_CPPFLAGS@ -DNOSHORTS -DARMA_NO_DEBUG=1
PKG_LIBS=@PKG_LIBS@

$(SHLIB): @CSDP_TS@

Csdp.ts:
	(cd Csdp/lib && \
	($(MAKE) libsdp.a CC="$(CC)" CFLAGS="-DDEFINE_FUNCTIONS_LAPACK_BLAS $(CLINK_CPPFLAGS) $(CFLAGS) $(CPICFLAGS) -DNOSHORTS -I../include" AR="$(AR)" RANLIB="$(RANLIB)") && \
	touch $@)


