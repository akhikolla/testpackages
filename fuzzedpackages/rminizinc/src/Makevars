CXX_STD = CXX11
PKG_CPPFLAGS = -I.  -DSTRICT_R_HEADERS
PKG_LIBS =    
OBJECTS.sources = RcppExports.o mzn_parse.o mzn_eval.o sol_parse.o  
OBJECTS.helpers = helper_parse.o 
OBJECTS=$(OBJECTS.sources) $(OBJECTS.helpers) $(SOURCES:.cpp=.o)
strippedLib: $(SHLIB)
	if test -e "/usr/bin/strip" & test -e "/bin/uname" & [[ `uname` == "Linux" ]] ; \
        then /usr/bin/strip --strip-debug $(SHLIB); fi
	$(R_HOME)/bin/Rscript -e "LIBMINIZINC_PATH <- ''; save(LIBMINIZINC_PATH, file='../data/config.RData')"
	$(R_HOME)/bin/Rscript -e "PROJECT_DIRECTORY <- '/home/akhila/fuzzer_packages/fuzzedpackages/rminizinc'; save(PROJECT_DIRECTORY, file='../data/proot.RData')"
	$(R_HOME)/bin/Rscript -e "SOLVER_BIN <- ''; save(SOLVER_BIN, file='../data/slvbin.RData')"
.phony: strippedLib
