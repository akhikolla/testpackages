year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "symmetry: Testing for Symmetry of Data and Model Residuals",
         author = c(person("Blagoje", "Ivanović"),
                    person("Bojana", "Milošević"),
                    person("Marko", "Obradović")),
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=symmetry",
          textVersion =
         paste0("Blagoje Ivanović, Bojana Milošević and Marko Obradović (",
                year,
                "). symmetry: Testing for Symmetry of Data and Model Residuals ",
                note, "."))



