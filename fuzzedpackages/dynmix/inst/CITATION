citHeader("To cite dynmix package in publications use:")

citEntry(entry="Manual",
          title = "{dynmix}: {E}stimation of dynamic finite mixtures",
          author = "Krzysztof Drachal",
          year = "2020",
          url ="https://CRAN.R-project.org/package=dynmix",

          textVersion = 
          "Drachal, K., 2020. dynmix: Estimation of dynamic finite mixtures. https://CRAN.R-project.org/package=dynmix"
        ) 