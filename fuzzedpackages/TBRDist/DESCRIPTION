Package: TBRDist
Title: Rearrangement Distances Between Unrooted Phylogenetic Trees
Version: 1.0.2
Authors@R: c(person("Martin R.", 'Smith',
                  email = "martin.smith@durham.ac.uk", 
                  role = c("aut", "cre", "cph"), 
                  comment = c(ORCID = "0000-0001-5660-1727")),
