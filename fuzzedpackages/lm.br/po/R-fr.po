msgid "'method' must be \"CLR\" or \"AF\""
msgstr "'method' doit être \"CLR\" ou \"AF\""

msgid "'incr' must be positive"
msgstr "'incr' doit être positif"

msgid "'output' must be \"G\", \"T\" or \"V\""
msgstr "'output' doit être \"G\", \"T\" ou \"V\""

msgid "mutiple response vectors not supported"
msgstr "plus d'un vecteur 'y' n'est pas permis"

msgid "zero variances not allowed"
msgstr "variances de zéro pas permis"

msgid "dim('weights') invalid"
msgstr "dim('weights') incorrect"

msgid "'weights' not positive-definite"
msgstr "'weights' n'est pas positive-définie"

msgid "'alpha'=0 not supported for type \"LL\""
msgstr "le cas 'alpha'=0 n'est pas permis pour type \"LL\""

msgid "'type' must be \"LL\", \"LT\" or \"TL\""
msgstr "'type' doit être \"LL\", \"LT\" ou \"TL\""

msgid "'rWy' must be a numeric vector"
msgstr "'rWy' doit être un vecteur numérique"

msgid "'output' must be \"T\", \"V\" or \"B\""
msgstr "'output' doit être \"T\", \"V\" ou \"B\""

msgid "'method' must be \"CLR\", \"AF\" or \"MC\""
msgstr "'method' doit être \"CLR\", \"AF\" ou \"MC\""
