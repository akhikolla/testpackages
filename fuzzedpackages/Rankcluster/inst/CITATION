citHeader("To cite Rankcluster, use:")

citEntry(
  header   = "Probabilistic Model and Algorithms:",
  entry    = "Article",
  title    = "A Generative Model for Rank Data based on Sorting Algorithm",
  author   = "Christophe Biernacki and Julien Jacques",
  journal  = "Computational Statistics and Data Analysis",
  year     = "2013",
  volume   = "58",
  pages    = "162--176",
  url      = "https://doi.org/10.1016/j.csda.2012.08.008",
  textVersion = paste("C. Biernacki and J. Jacques (2013), A generative model for rank data based on sorting algorithm, Computational Statistics and Data Analysis, 58, 162-176.")
)

citEntry(
  header   = "R package:",
  entry    = "Article",
  title    = "Rankcluster: An R Package for Clustering Multivariate Partial Rankings",
  author   = "Julien Jacques and Quentin Grimonprez and Christophe
          Biernacki",
  journal  = "The R Journal",
  year     = "2014",
  volume   = "6",
  number   = "1",
  pages    = "101--110",
  url      = "https://doi.org/10.32614/RJ-2014-010",
  textVersion = paste("J. Jacques, Q. Grimonprez and C. Biernacki (2014) Rankcluster: An R Package for Clustering Multivariate Partial Rankings, The R Journal (2014) 6:1, 101-110.")
)
