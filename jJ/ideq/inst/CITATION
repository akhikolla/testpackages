citHeader("To cite ideq in publications use:")

citEntry(entry = "Manual",
  title        = "ideq: An R Package for Dynamic Spatio-temporal Models",
  author       = personList(as.person("Easton Huch"),
                   as.person("Robert Richardson")),
  organization = "Brigham Young University",
  year         = "2019",

  textVersion  =
  paste("Easton Huch, Robert Richardson (2019).",
        "ideq: An R Package for Dynamic Spatio-temporal Models.",
        "Brigham Young University.")
)